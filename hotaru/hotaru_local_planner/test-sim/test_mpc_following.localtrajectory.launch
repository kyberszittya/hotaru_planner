<launch>
    <param name="use_sim_time" value="true" />
    <include file="$(find szelectricity_gazebo_plugins)/launch/gpsfix2pos.abs.launch" />
    <include file="$(find nissan_bringup)/launch/nissan.leaf.staticbringup.ref.launch" />

    <node pkg="leaf_tf_publisher" type="leaf_tf_publisher" name="leaf_tf_publisher">
        <remap from="gnss_pose" to="current_pose"/>
        <remap from="gps/current_pose" to="gnss_pose"/>
    </node>
    <node pkg="tf" type="static_transform_publisher" name="gps_to_baselink_static_tf" args="0 0 0 0 0 0 gps base_link 100"/>
    
    <node pkg="waypoint_maker" type="waypoint_marker_publisher" name="waypoint_marker_publisher" />
    
</launch>
