<hybridsystem>
	<statemachine>
		<name>parking_scenario</name>
		<locations>
			<location>Idle</location>
			<location>Searching</location>
			<location>Parking</location>
			<location>OptimizeParking</location>
		</locations>
		<start_location>
			<location>Idle</location>
			<event>SwitchBehaviorSignal</event>
		</start_location>
		<end_location>
			<location>Idle</location>
			<event>ExitBehaviorSignal</event>
		</end_location>
		<transitions>
			<transition>
				<source>Idle</source>
				<target>Searching</target>
				<event>ParkingStartSignal</event>
			</transition>
			<transition>
				<source>Searching</source>
				<target>Idle</target>
				<event>InfeasibleParkingPoint</event>
			</transition>
			<transition>
				<source>Searching</source>
				<target>Parking</target>
				<event>ParkingPointDetectedSignal</event>
			</transition>
			<transition>
				<source>Parking</source>
				<target>OptimizeParking</target>
				<event>ParkingPointCloseProximitySIgnal</event>
			</transition>
			<transition>
				<source>OptimizeParking</source>
				<target>Parking</target>
				<event>UnoptimalParkingSignal</event>
			</transition>
			<transition>
				<source>OptimizeParking</source>
				<target>Idle</target>
				<event>OptimalParkingSignal</event>
			</transition>
		</transitions>
	</statemachine>
</hybridsystem>