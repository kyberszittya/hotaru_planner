<hybridsystem>
	<statemachine>
		<name>parking_scenario</name>
		<delta_timeout>1000000000</delta_timeout>
		<locations>
			<location>
				<name>Idle</name>
			</location>
			<location>
				<name>Searching</name>
			</location>
			<location>
				<name>Parking</name>
			</location>
			<location>
				<name>OptimizeParking</name>
			</location>
		</locations>
		<start_location>
			<location>Idle</location>
			<event>SwitchBehaviorSignal</event>
		</start_location>
		<end_location>
			<location>Idle</location>
			<event>ExitBehaviorSignal</event>
		</end_location>
		<transitions>
			<transition>
				<source>Idle</source>
				<target>Searching</target>
				<discreteevent>ParkingStartSignal</discreteevent>
			</transition>
			<transition>
				<source>Searching</source>
				<target>Idle</target>
				<discreteevent>InfeasibleParkingPoint</discreteevent>
			</transition>
			<transition>
				<source>Searching</source>
				<target>Parking</target>
				<discreteevent>ParkingPointDetectedSignal</discreteevent>
			</transition>
			<transition>
				<source>Parking</source>
				<target>OptimizeParking</target>
				<discreteevent>ParkingPointCloseProximitySignal</discreteevent>
			</transition>
			<transition>
				<source>OptimizeParking</source>
				<target>Parking</target>
				<discreteevent>UnoptimalParkingSignal</discreteevent>
			</transition>
			<transition>
				<source>OptimizeParking</source>
				<target>Idle</target>
				<discreteevent>OptimalParkingSignal</discreteevent>
			</transition>
		</transitions>
	</statemachine>
</hybridsystem>